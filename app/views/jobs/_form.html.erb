<%= simple_form_for job do |f| %>
      <%= f.input :name, input_html: { class: 'form-control input-sm'}  %>
      <%= f.input :building, input_html: { class: 'form-control input-sm'}  %>
      <%= f.input :address, input_html: { class: 'form-control input-sm'} %>
      <%= f.input :city, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :state, input_html: { class: 'form-control input-sm'} %>
      <%= f.input :zip, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :country, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :project_phase, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :project_type, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :sachse_group, input_html: { class: 'form-control input-sm'} %>
      <%= f.association :contract_type, input_html: { class: 'form-control input-sm'} %>
      <%= f.input :sqft, input_html: { class: 'form-control input-sm'} %>
      
      <%= f. :turnover_date, input_html: { id: 'datepicker'} %>
      <%= f.submit "Save", input_html: { class: 'btn btn-success'} { rows="10"} %>
<% end %>


